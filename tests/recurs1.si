algorithm inc(input uint8 i,output uint8 o)
{
$$print('N = ' .. widthof('N'))
$$if widthof('N') > 0 then
  inc i1<N:uint$widthof('N')-1$>;
  (o) <- i1 <- (i+1);
$$else
  o = i;  
$$end  
}

unit main(output uint8 leds)
{
  inc i0<N:uint8>;

  algorithm {
 
    (leds) <- i0 <- (5);
    
     __display("%d\n",leds);
 
  }

}