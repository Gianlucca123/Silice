
// integer division
$include('../common/divint_std_auto.si')

algorithm main(output uint8 leds)
{
  int16  num    =   20043;
  int16  den    = - 41;
  int16  result = 0;

  div div0<inum:int16>;

  (result) <- div0 <- (num,den);

  __display("%d / %d = %d",num,den,result);

  leds = result[0,8];

}
